type AssetAdministrationShell {
  idShort: String
  version: String
  revision: String
  aasId: String 
  displayName: String
  description: String
  checksum: String
  assetInformation: AssetInformation
  dataId: ID!

  # Section to add submodules that have been addes to this schema
  # e.g. contactInformations: ContactInformations
}

type AssetInformation {
  dataId: ID!
  assetKind: AssetKind
  globalAssetId: String!
  specificAssetId: [SpecificAssetId!]!
}

enum AssetKind {
  Type
  Instance
}

type SpecificAssetId {
  dataId: ID!
  name: String
  value: String
}

type MetaData {
  metaId: ID!
  metaType: metaType
}

enum metaType {
  # siehe Details of the AAS Part1 Seite 84
  FragmentReference
  Referable
  AnnotatedRelationshipElement
  BasicEventElement
  Blob
  Capability
  DataElement
  Entity
  BasicEventElementFile
  MultiLanguageProperty
  Property
  Operation
  Range
  RelationshipElement
  ReferenceElement
  SubmodelElement
  SubmodelElementCollection
  SubmodelElementList
  AssetAdministrationShell
  ConceptDescription
  Identifiable
  Submodel
  GlobalReference
}

# nicht IDTA
type Counter {
  fromDataId: ID!
  toDataId: ID!
  counter: String
}
